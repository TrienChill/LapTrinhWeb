
@{
    ViewBag.Title = "AddPageF";
    Layout = "~/Views/Layout/_LayoutPage.cshtml";
}
<style>
    #Title {font-size: 18px; width: 570px; height: 160px;}
    #Sumary {font-size: 18px; width: 570px; height: 160px;}
    #ID_Parent {font-size: 18px;}
    #ListOut tr td b {font-size: 20px;}
    #ID_P {font-size: 17px;}
    button {font-size: 17px;text-decoration: none;}
    .link {text-decoration: none;}
    #OrderKey {font-size: 18px;}
    td {font-size: 20px;}
    h2 {margin: 20px;}
    #file{font-size: 18px;}
    table {margin-left: 20px;}
</style>
@model NEWSMODELS.Models.PageFooter
@using (Html.BeginForm("AddPageF", "PageFooter", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
<table>
    <tr><td>ID_F :</td><td><b>@Html.TextBox("ID_F", (object)ViewBag.idpnew)</b></td></tr>
    <tr><td>Title :</td><td><b>@Html.TextBox("TitleF")</b></td></tr>
    <tr><td>Content:</td><td><b>@Html.TextArea("ContentF")</b></td></tr>
    <tr>
        <td>Menu bài viết:</td>
        <td>@Html.DropDownList("ParentID", new SelectList(ViewBag.parents, "ID_Footer", "TitleFooter", ViewBag.parent), "chọn menu")</td>
    </tr>
    <tr>
        <td></td>
        <b class="note">@Html.Raw(ViewBag.Message)</b>
        <br />
        <td>
            <button type="submit"><i class="fa fa-plus"></i>Thêm mới</button>
        </td>
    </tr>
</table>
}